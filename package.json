{
  "name": "discord-img-bot",
  "version": "1.0.0",
  "description": "A Discord bot for creating image backups",
  "main": "dist/app/bot.js",
  "scripts": {
    "start": "ts-node . --log-level=verbose --log-flag=date",
    "dev": "tsc-watch --project . --outDir ./dist --onSuccess \"ts-node . --log-level=verbose --log-flag=date\"",
    "clean": "rm -rf node_modules dist",
    "deps:update": "npm-check-updates -u"
  },
  "author": "s1njar.mail@gmail.com",
  "dependencies": {
    "@guanghechen/chalk-logger": "6.0.0-alpha.6",
    "chalk": "^5.3.0",
    "discord-fetch-all": "^3.0.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "string-argv": "^0.3.2",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsc-watch": "^6.0.4",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2",
    "npm-check-updates": "^16.14.15"
  }
}
