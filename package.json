{
  "name": "back-boner-bot",
  "version": "1.0.0",
  "description": "Bot for creating images.",
  "main": "dist/app/bot.js",
  "scripts": {
    "prebuild": "rm -rf dist/* && tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc && cp -rf package.json dist/package.json",
    "prestart": "npm run build",
    "start": "ts-node .",
    "watch": "tsc-watch --project . --outDir ./dist --onSuccess \"ts-node . --log-level=verbose --log-flag=date\""
  },
  "author": "s1njar.mail@gmail.com",
  "dependencies": {
    "@barusu/chalk-logger": "0.0.35",
    "chalk": "^4.1.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node-fetch": "^2.5.8",
    "@types/ws": "^7.4.0",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
